    "react-dom": "^17.0.1",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "nyc": {
    "extensions": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.tsx?"
    ],
    "exclude": [
      "__test__"
    ]
  },
  "dependencies": {
    "rxjs": "^6.6.3"
